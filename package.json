{
  "name": "m-report",
  "sideEffects": [
    "*.css",
    "*.scss",
    "*.less",
    "*.styl"
  ],
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev:build": "cross-env NODE_ENV=development webpack --config ./build/webpack.dev.js",
    "dev:analysis": "cross-env NODE_ENV=development webpack --config ./build/webpack.dev.js --profile --json > stats.json",
    "dev:server:analyzer": "cross-env NODE_ENV=development webpack-dev-server --config ./build/webpack.dev.js --inline --progress --devserver --analyzer",
    "dev:server": "cross-env NODE_ENV=development webpack-dev-server --config ./build/webpack.dev.js",
    "dev:express:server": "cross-env NODE_ENV=development node ./server/index.js",
    "build": "cross-env NODE_ENV=production webpack --config ./build/webpack.prod.js",
    "build:dll:dev": "cross-env NODE_ENV=development webpack --config ./build/webpack.dll.js",
    "build:dll:prod": "cross-env NODE_ENV=production webpack --config ./build/webpack.dll.js",
    "prod:express:server": "cross-env NODE_ENV=production node ./server/index.js"
  },
  "author": "haoran",
  "license": "ISC",
